program XLSample1 =
	// correct output is:	3	-1	2187
	constant one : Integer := 1;
	constant two : Integer := 2;
	var y, x1, x2, x3, x4, x5, x6, x7, x8, x9, x10, x11, x12, x13, x14 : Integer;

begin // XLSample1
	x1  := one;
	x2  := two;
	x3  := one;
	x4  := two;
	x5  := one;
	x6  := two;
	x7  := one;
	x8  := two;
	x9  := one;
	x10 := two;
	x11 := one;
	x12 := two;
	x13 := one;
	x14 := two;
	y := (x1+x2)*((x3+x4)*((x5+x6)*((x7+x8)*((x9+x10)*((x11+x12)*(x13+x14))))));
	put(one + two);
	put(-1);
	newLine;
	put(y);
	newLine;
end XLSample1.
